<aura:component
  implements="forceCommunity:themeLayout"
  access="global"
  description="tmxTheme"
>
  <aura:attribute name="newHeader" type="Aura.Component[]" required="false" />
  <aura:attribute name="themeHeader" type="Aura.Component[]" required="false" />
  <aura:attribute name="themeFooter" type="Aura.Component[]" required="false" />

  <div>
    <div class="themeHeader">{!v.themeHeader}</div>
    <div class="newHeader">{!v.newHeader}</div>
    <div class="mainContentArea">{!v.body}</div>
    <div class="themeFooter">{!v.themeFooter}</div>
  </div>
</aura:component>